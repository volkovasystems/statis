  },
  "homepage": "https://github.com/volkovasystems/statis#readme",
  "dependencies": {
    "arid": "^0.25.0",
    "babel-runtime": "^6.23.0",
    "diatom": "^0.27.0",
    "falzy": "^0.19.0",
    "harden": "^0.21.0",
    "protype": "^0.21.0",
    "raze": "^0.29.0",
